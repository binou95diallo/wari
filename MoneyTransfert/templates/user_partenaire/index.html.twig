    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Matricule</th>
                <th>Username</th>
                <th>Password</th>
                <th>NomComplet</th>
                <th>Adresse</th>
                <th>Telephone</th>
                <th>Email</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for user_partenaire in user_partenaires %}
