                <td>{{ user_partenaire.adresse }}</td>
                <td>{{ user_partenaire.telephone }}</td>
                <td>{{ user_partenaire.email }}</td>
                <td>
                    <a href="{{ path('user_partenaire_show', {'id': user_partenaire.id}) }}">show</a>
                    <a href="{{ path('user_partenaire_edit', {'id': user_partenaire.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="9">no records found</td>
            </tr>
        {% endfor %}
